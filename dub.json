{
	"description": "cairo bindings and wrappers for the D programming language.",
	"targetName": "cairoD",
	"authors": [
		"Johannes Pfau",
		"Andrej Mitrovic"
	],
	"homepage": "https://github.com/cairoD/cairoD",
	"name": "cairod",
	"copyright": "Copyright Â© 2011-2015, Johannes Pfau",
	"license": "BSL-1.0",
	"-ddoxFilterArgs": ["--unittest-examples"],
	"dependencies": {
		"derelict-ft": "~>1.0.2",
	},
	"configurations": [
		{
            "name": "static-custom-windows",
            "targetType": "staticLibrary",
            "platforms": ["windows"],
            "dependencies": {"windows-headers": "~>1.0.1"}
        },
        {
            "name": "static-custom-posix",
            "targetType": "staticLibrary",
            "platforms": ["posix"],
            "dependencies": {"x11": "~>1.0.9"}
        },
        {
            "name": "unittest",
            "targetType": "executable",
            "sourceFiles": ["unittest.d"],
            "libs": ["cairo"],
            "versions": ["CairoPNG"]
        }
	]
}
